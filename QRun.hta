	// '*********************************************************************************************
	// !расположение и размер окна
	// '*********************************************************************************************

	var winWidth = 1000; // ширина
	var winHeight = 800; // высота
	window.resizeTo(winWidth, winHeight);

	// окно в центр экрана
	var winPosX = screen.width / 2 - winWidth / 2;
	var winPosY = (screen.height / 2 - winHeight / 2) * 0.7;
	window.moveTo(winPosX, winPosY);
